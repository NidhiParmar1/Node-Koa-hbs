<h3>List of Places</h3>
<div class="alert alert-success alert-dismissible">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <strong><%- msg %></strong> 
  </div>
<div class="container">
<table class="table table-bordered">
     <thead><th>Place</th>
          <th>PlaceStatus</th>
          <th>Owner Id</th>
          <th>Approve</th>
          <th>Reject</th>
          </thead>
     
      <% adata.forEach(data=>{ %>
      <tr>
       <td>
        <%= data.place %>
         </td>
         <td>
        <%= data.placestatus %>
         </td>
         <td>
        <%= data.ownerid %>
         </td>
         <td><a class= "btn" href="approveplace?id=<%-data._id %>" onclick="return confirm('Do you want to Approve this place?')">Approve</a></td>
         <td><a class= "btn" href="rejectplace?id=<%- data.id %>" onclick="return confirm('Do you want to Reject this place?')">Reject</a></td>
         </tr>
    <% }) %>
    
    </table>
</div>